{
  "name": "weather-api",
  "version": "0.0.0",
  "engines": {
    "node": ">=8.9.1 <9.0.0",
    "yarn": ">=1.1.0 <2.0.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "clean": "n-clean dist temp coverage",
    "build": "n-clean dist && ng build --prod",
    "test": "ng test",
    "lint": "prettier --config ./prettier.config.js --list-different \"{src,e2e}/{app,environments,assets,pageobjects,specs}/**/*{.ts,.js,.json}\" && ng lint",
    "e2e": "ng e2e",
    "mock": "concurrently -k \"node mocks/mock.server.js\" \"yarn start:mock\"",
    "start:mock": "yarn start && ng serve --proxy-config mocks/proxy.conf.js --serve-path /weatherAPIHost/ --live-reload false",
    "build:analyze": "yarn build --stats-json && webpack-bundle-analyzer dist/stats.json",
    "test:console": "ng test --source-map false",
    "test:coverage": "ng test --watch=false --code-coverage",
    "lint:fix": "prettier --write --config ./prettier.config.js --list-different \"{src,e2e}/{app,environments,assets,pageobjects,specs}/**/*{.ts,.js,.json}\"",
    "pre-push": "yarn clean && yarn lint && yarn start && yarn test:coverage && yarn build",
    "fetch": "yarn --pure-lockfile",
    "yarn:after-add": "node bin/yarn-after-add.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.2",
    "@angular/cdk": "^6.4.3",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/material": "^6.4.3",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.0.3",
    "bootstrap": "^4.1.1",
    "core-js": "^2.5.4",
    "cors": "latest",
    "jsoneditor": "^5.33.0",
    "ngx-spinner": "^6.1.2",
    "rxjs": "6.0.0",
    "yarn": "latest",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular/cli": "~6.0.8",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/language-service": "^6.0.3",
    "@types/jasmine": "~2.5.38",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash-es": "^4.17.1",
    "@types/node": "~8.9.4",
    "body-parser": "^1.18.3",
    "codelyzer": "~4.2.1",
    "concurrently": "^3.6.0",
    "express": "^4.16.3",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha-reporter": "^2.2.5",
    "npm-build-tools": "^2.2.5",
    "prettier": "1.13.7",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "^2.7.2"
  }
}
